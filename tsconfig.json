{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@common/*": ["src/common/*"],
      "@config/*": ["src/config/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}